{
  "name": "@imgsplit/cli",
  "version": "0.0.1",
  "type": "module",
  "author": "h1ve2",
  "license": "MIT",
  "homepage": "https://imgsplit.github.io/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imgsplit/cli.git"
  },
  "bugs": {
    "url": "https://github.com/imgsplit/cli/issues"
  },
  "keywords": [
    "imagesplit",
    "imagesplitter",
    "cli"
  ],
  "description": "Command-line tool for ImgSplit",
  "scripts": {
    "build": "yarn run clean && rollup -c",
    "start": "yarn run clean && rollup -c --watch",
    "clean": "rimraf ./dist",
    "prepublishOnly": "yarn run build",
    "publish-ci": "yarn publish",
    "test": "rimraf ./test/.out && vitest run"
  },
  "main": "./dist/index.js",
  "bin": {
    "imgsplit": "./dist/index.js"
  },
  "files": [
    "dist/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/node": "^22.15.18",
    "@types/yargs": "^17.0.33",
    "rimraf": "^6.0.1",
    "rollup": "^4.40.2",
    "tslib": "^2.8.1",
    "typescript": "^5.6.3",
    "vitest": "^3.1.3"
  },
  "dependencies": {
    "@imgsplit/core": "^0.1.1",
    "ora": "^8.2.0",
    "upath": "^2.0.1",
    "yargs": "^17.7.2"
  }
}
